@article{ WOS:000733862200001,
Author = {Danilevicz, Vatsi Meneghel and Ferreri, Marcelo Almeida},
Title = {Shapeless faces and invisible images},
Journal = {INTERFACE-COMUNICACAO SAUDE EDUCACAO},
Year = {2022},
Volume = {26},
Abstract = {Considering that subjectivation processes directly affect the mental
   health of populations, we pursue lines of flight to subvert systems of
   oppression by problematizing ``the abstract-machine of faciality{''}. To
   this end, we deface standard faciality into fragments of becomings,
   pieces of imperceptible multiplicities, latent forces of an everyday
   that wants movement and, paradoxically, resists it. The method is an
   installation, where images alternate with philosophical and literary
   passages, making up a mosaic of intelligibility that mixes fiction and
   non-fiction. With regard to images, we make use of two main editing
   resources: transparency and overlay, which dialogue directly with the
   objective of undoing standard faciality with the subtleness and force of
   becomings. The corpus of study unfolds into three parts: becoming
   landing-between, becoming-flames, and, finally, becoming-tides.},
