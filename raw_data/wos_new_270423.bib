@article{ WOS:000464580400008,
Author = {Maia de Araujo, Luis Paulo and Marques, Rodrigo Moreno},
Title = {An analysis on the active transparency in websites of the Brazilian
   Federal Executive Power},
Journal = {REVISTA IBERO-AMERICANA DE CIENCIA DA INFORMACAO},
Year = {2019},
Volume = {12},
Number = {2},
